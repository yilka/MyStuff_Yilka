<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MyStuff_Yilka.Views.AgregarBrandPage">
    <ContentPage.Content>
        <StackLayout BackgroundColor="Black" Padding="20, 50, 20, 50">
            <Entry x:Name="TxtBrand" Placeholder="Brand" FontSize="Large" TextColor="White" FontAttributes="Bold" PlaceholderColor="#B2b2b2" Keyboard="Text" />

            <Button x:Name="BtnAceptar" Text="Aceptar" BackgroundColor="#DEF3CF" TextColor="Black" FontAttributes="Bold" Clicked="BtnAceptar_Clicked" />
            <Button x:Name="BtnCancelar" Text="Cancelar" BackgroundColor="#F3D2CF" TextColor="Black" Clicked="BtnCancelar_Clicked"/>

            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>